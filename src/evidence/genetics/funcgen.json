{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "OpenTargets POSTGAP funcgen",
  "description": "OpenTargets POSTGAP funcgen. This captures the functional genomics scores from POSTGAP",
  "type": "object",
  "version": "1.2.7",
  "allOf": [
    {
      "$ref": "https://raw.githubusercontent.com/opentargets/json_schema/postgap/src/evidence/base.json"
    },
    {
      "properties": {
       	"postgap_score": {
           "type": "number",
           "minimum": 0
        },
        "postgap_rank": {
           "type": "number",
           "minimum": 0
        },
        "vep_sum": {
           "type": "number",
           "minimum": 0
        },
        "vep_mean": {
           "type": "number",
           "minimum": 0
        },
        "gtex_score": {
           "type": "number",
           "minimum": 0
        },
        "vep_score": {
           "type": "number",
           "minimum": 0
        },
        "fantom5_score": {
           "type": "number",
           "minimum": 0
        },
        "dhs_score": {
           "type": "number",
           "minimum": 0
        },
        "pchic_score": {
           "type": "number",
           "minimum": 0
        },
        "is_nearest_gene": {
           "type": "boolean"
        },
        "regulome_score": {
           "type": "number",
           "minimum": 0
        },
        "vep_reg_score": {
           "type": "number",
           "minimum": 0
        }
     }
   }
  ],
  "properties": {
    "postgap_score": {},
    "postgap_rank": {},
    "vep_terms": {},
    "vep_sum": {},
    "vep_mean": {},
    "gtex_score": {},
    "vep_score": {},
    "fantom5_score": {},
    "dhs_score": {},
    "pchic_score": {},
    "is_nearest_gene": {},
    "regulome_score": {},
    "vep_reg_score": {}
  },
  "required": [
    "postgap_score",
    "postgap_rank"
  ],
  "additionalProperties": false
}
