{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Rank",
  "description": "Rank",

  "type": "object",
  "version": "1.2.2",
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "rank"
      ]
    },
    "position": {
      "type": "number",
      "minimum": 1
    },
    "sample_size": {
      "type": "number",
      "minimum": 1
    },
    "method": {
      "type": "object",
      "$ref": "method.json"
    }
  },
  "additionalProperties": false,
  "required": [
    "position",
    "sample_size",
    "type"
  ]
}