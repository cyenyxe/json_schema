{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "file:///home/eddturner/working/cttv/repos/json_schema/src/bioentity/phenotype.json",
  "title": "phenotype",
  "description": "CTTV phenotype model",
  "type": "object",
  "version": "1.2.2",
  "allOf": [
    {
      "$ref": "base.json"
    },
    {
      "properties": {
        "term_id": {
          "type": "string",
          "description": "Phenotype term Identifier from HPO/MP",
          "pattern": "^http://purl.obolibrary.org/obo/HP_[0-9]{4,}||http://purl.obolibrary.org/obo/MP_[0-9]{4,}$"
        },
        "label": {
          "type": "string",
          "description": "Phenotype term label"
        },
        "species": {
          "type": "string",
          "enum": [
            "mouse",
            "human",
            "rat",
            "zebrafish",
            "dog"
          ]
        }
      }
    }
  ],
  "properties": {
    "id": {},
    "term_id": {},
    "label": {},
    "species": {}
  },
  "required": [
    "id",
    "term_id",
    "label",
    "species"
  ],
  "additionalProperties": false
}
