{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Variant",
    "description": "OpenTargets variant model - an array of dbSNP, European structural variant (esv) or NCBI structural variant (nsv)",
    "type": "object",
    "version": "1.2.7",
    "allOf": [
        {
            "$ref": "https://raw.githubusercontent.com/opentargets/json_schema/postgap/src/bioentity/base.json"
        },
        {
            "properties": {
                "id": {
                    "type": "string",
                    "description": "An array of variant identifiers",
		    "pattern": "^http://www.ncbi.nlm.nih.gov/clinvar/RCV[0-9]{9}|http://identifiers.org/dbsnp/rs[0-9]{1,}|http://identifiers.org/dbsnp/esv[0-9]{1,}|http://identifiers.org/dbsnp/nsv[0-9]{1,}$"
                },
                "type": {
                    "type": "string",
                    "enum": [
                        "snp single",
                        "snp snp interaction",
                        "structural variant"
                    ]
                },
                "is_gwas_snp": {
                    "type": "boolean"
                },
		"chrom": {
                    "type": "string"
                },
		"pos": {
                    "type": "number",
		    "minimum": 0,
	   	    "exclusiveMinimum": true
                },  
		"cluster_id": {
                    "type": "string"
                }  
            }
        }
    ],
    "properties": {
        "id": {},
        "type": {},
	"is_gwas_snp": {},
	"chrom": {},
	"pos": {},
	"cluster_id": {}
    },
    "required": [
        "id",
        "type"
    ],
    "additionalProperties": false
}
